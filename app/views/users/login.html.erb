<h1>Login</h1>
<p>Please enter your credential</p>

<%= form_for(@user) do |f| %>
   
 
  <div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>

    <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %><br />
    
    
    <%= f.submit  :logged, :url => logged_path %>
    
<% end %>


