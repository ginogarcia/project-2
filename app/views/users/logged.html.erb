<h1>You are Logged</h1>
<p id="notice"><%= notice %></p>

<%= form_for(:logged, :url => logged_path) do |f| %>
   
 
  <div class="field">
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>

    <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password %><br />
    <%= f.submit %>
    
<% end %>

